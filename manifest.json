{
    "name": "Bookmark",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "scripting",
        "activeTab",
        "tabs"
    ],
    "background": {
        "service_worker": "src/js/background.js"
    },
    "action": {
        "default_popup": "src/html/popup.html"
    },
    "commands": {}
}